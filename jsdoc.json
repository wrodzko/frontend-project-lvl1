{
    "source": {
        "include": ["./src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "title": "Brain Games Documentation",
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./readme.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/better-docs"
    }
}